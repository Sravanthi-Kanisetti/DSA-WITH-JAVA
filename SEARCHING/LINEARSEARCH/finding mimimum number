
public class mina{
    public static void main(String[] args){
        int[] nums={2,10,5,23,89,55,110,49,76,20};
        int result=minimum(nums);
        System.out.println("the minimum element in list is=> "+result);
    }
    
    
    static  int minimum(int[] nums){
     int fr=nums[0];
     for(int i=1;i<nums.length;i++){
         if(nums[i]<fr){
             fr=nums[i];
         }
     }
     
     return fr;
    }
}
